{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "connect-src 'self' https://accounts.google.com https://*.googleapis.com https://code-snippet-manager-oowo.vercel.app"
        }
      ]
    }
  ]
}